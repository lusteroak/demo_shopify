<h2>
  {{ collections['frontpage'].title }}
</h2>

{% for product in collections['frontpage'].products %}
  <div>
    <a href="{{ product.url  }}">
      <h3>{{ product.title }}</h3>
      <p>{{ product.price | money }}</p>
      <img src="{{ product.featured_image | image_url: width: 450 }}" />
    </a>
  </div>
  {{- product.title | link_to: product.url }}
{% endfor %}